{
  "expo": {
    "name": "WHS",
    "slug": "whs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "whs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey":  "process.env.GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleMaps": {
        "apiKey": "process.env.GOOGLE_MAPS_API_KEY"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.altronx.whs"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/impact.ttf",
        "name": "Impact"
      },
      {
        "asset": "./assets/fonts/Roboto-Regular.ttf",
        "name": "Roboto-Regular"
      },
      {
        "asset": "./assets/fonts/Roboto-SemiBold.ttf",
        "name": "Roboto-SemiBold"
      },
      {
        "asset": "./assets/fonts/Roboto-Italic.ttf",
        "name": "Roboto-Italic"
      }
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "76eacce6-23be-400e-81f1-ee2ae50c5ec3"
      }
    },
    "owner": "altronx"
  }
}
